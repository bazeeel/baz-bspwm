!Xdefaults --

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintfull
!hintmedium hintslight hintfull
Xft.lcdfilter: lcddefault
Xcursor.theme: Bibata_Oil

!URxvt ---------------------------------------------------------------------- !

!Fonts --

URxvt.font: xft:JetBrains Mono:size=10,xft:JetBrains Mono Medium:size=9
URxvt.boldFont:
URxvt.letterSpace: 1
URxvt.antialias: true

!Main Settings --

URxvt.termname: xterm-256color
URxvt.iconFile: /usr/share/icons/Papirus/48x48/apps/urxvt.svg

URxvt.cursorUnderline: false
URxvt.pointerBlank: true
URxvt.saveLines:    5000
URxvt.scrollBar:    false
URxvt.cursorBlink:  true
URxvt.urgentOnBell: true
URxvt*scrollTtyOutput: true
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

!Extensions --

URxvt.perl-ext: default,clipboard,url-select,keyboard-select

!Tabs --

!URxvt.tabbed.tabbar-fg: 7
!URxvt.tabbed.tabbar-bg: 0
!URxvt.tabbed.tab-fg: 7
!URxvt.tabbed.tab-bg: 0

!URL Launcher --

URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.colorUL: #D9A9A2

!Disable iso14755 --

URxvt.iso14755: false
URxvt.iso14755_52: false

!Clipboard --

URxvt.clipboard.autocopy: true
URxvt.keysym.Alt-c: perl:clipboard:copy
URxvt.keysym.Alt-v: perl:clipboard:paste


!Control + arrow navigation --

URxvt.keysym.Control-Up:\033[1;5A
URxvt.keysym.Control-Down:\033[1;5B
URxvt.keysym.Control-Left:\033[1;5D
URxvt.keysym.Control-Right:\033[1;5C

!URxvt window --

URxvt*geometry: 80x20
URxvt.internalBorder: 20

!Colors --

urxvt*depth: 32


!*background:  rgba:0000/0000/0200/c800


*foreground:  #FFFFFF
*color0:  #3e3e3e
*color1:  #dd5a3a
*color2:  #48a37f
*color3:  #fe9b3d
*color4:  #1858a5
*color5:  #e58da3
*color6:  #3697b9
*color7:  #f8e9f2
*color8:  #4e4e4e
*color9:  #f0544c
*color10:  #0bbaa7
*color11:  #f5b427
*color12:  #3caed9
*color13:  #fa96a3
*color14:  #84c4ee
*color15:  #ffffff

!St configuration

!! Transparency (0-1):
st.alpha: 100

!! Set a default font and font size as below:
st.font: Monospace-12;

! st.termname: st-256color
! st.borderpx: 2

!! Set the background, foreground and cursor colors as below:
*.background: #2f2b26
*.foreground: white
*.cursorColor: white

